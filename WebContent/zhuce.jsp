<body>

<%@include file="taitou.jsp" %>
<%
String reStr="";
if(request.getAttribute("zhuce")!=null){
    reStr=(String)request.getAttribute("zhuce");
}
%>

<script type="text/javascript">
var codeFlag=false;
function checkpw(){
	  var pw1=document.getElementById("pw1").value;
	  var pw2=document.getElementById("pw2").value;
	  if(pw1!=pw2 || pw1==""){
		  document.getElementById("tishi").innerHTML="<font color='red'>两次密码不相同</font>";
		  document.getElementById("submit").disabled=true;
	  }
	  else{
		  document.getElementById("tishi").innerHTML="";
		  document.getElementById("submit").disabled=false;
	  }
}
function checkName(){
    var cname=document.getElementById("cname").value;
    if(cname==""){
        document.getElementById("tishiName").innerHTML="请输入用户名";
        document.getElementById("submit").disabled=true;
    }
    else{
        document.getElementById("tishiName").innerHTML="";
        document.getElementById("submit").disabled=false;
    }
}
function butcheck(){

    if(!codeFlag){
    	document.getElementById("submit").disabled=true;
    	alert("请进行滑块验证");
    }else{
    	document.getElementById("submit").disabled=false;
    }
    checkName();
}
</script>

<!--会员注册开始-->

<div class="huiyuan_zc">
<div class="zhuce_1">用户注册</div>
<div class="zhuce_2" height="900px">

<div class="zhuce_left" style="position:relative;" >
<form action="UserServlet?method=zhuce" method="post">
	<div class="yhm_zc">用  户  名：<input id="cname" type="text" name="userName"  class="zc_wbk" onblur="checkName()" />
	<span id="tishiName"><font color='red'><%=reStr %></font></span>
	</div>
	<div class="yhm_zc">输入密码：<input id="pw1" type="password" name="passWord"  class="zc_wbk" /></div>
	<div class="yhm_zc">再次输入：<input id="pw2" type="password" name="passWord"  class="zc_wbk" onblur="checkpw()"/>
	   <span id="tishi"></span>
	</div>
	<!-- <div class="yhm_zc"><span style="float:left;">&nbsp;&nbsp;&nbsp;验证码：<input type="text" name="textfield"  class="zc_wbk" / style="width:100px;"></span><span style="float:left; width:132px; height:32px; display:block; margin-left:5px; margin-top:5px;">
	   <img src="images/yzm.jpg" width="120" height="32" /></span><span style="float:left;"><a href="#">看不清，再换一张</a></span>
	</div> -->
	
	<div class="zhuce_tjxx" onmouseover="butcheck()">
	    <input id="submit" type="submit" disabled="disabled"  value="" onmousedown="this.style.left='1px';this.style.top='1px';" onmouseup="this.style.left='0px';this.style.top='0px';"   / style="width:206px; height:35px ; border:none; background: url(images/zhuce_1.jpg); position:relative;">
	</div>
</form>
</div>



<div class="zhuce_right">

<div class="zhuce_right1"><a href="denglu.jsp"><img src="images/dll.jpg" width="149" height="78" / style="margin-left:50px; margin-top:20px;"></a></div>

<div class="zhuce_right2"><img src="images/bqsy.jpg" width="228" height="209" /></div>


</div>
<div id="slideBar" class="zhuce_right" style="left:100px;"></div>
    <script type="text/javascript">
        var dataList = ["0","1"];
        var options = {
            dataList: dataList,
            success:function(){  
                codeFlag=true;

            },
            fail: function(){
                codeFlag=false;

            }
        };
        SliderBar("slideBar", options);

    </script>






</div>


</div>

<!--会员注册结束-->


