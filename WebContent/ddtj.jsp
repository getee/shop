<c:set var="num" value="${0 }"></c:set>
<c:set var="money" value="${0 }"></c:set>
<c:if test="${sessionScope.ShoppingCar != null }">
<c:forEach var="wine" items="${sessionScope.ShoppingCar.keySet()}">

	<div class="dingd_sl">
	<table width="0" border="0" cellspacing="0" cellpadding="0">
	  <tr style=" font-family:微软雅黑; text-align:center; font-size:14px;">
	    <td width="43" height="87"><input name="" type="checkbox" value="" / style=" margin-left:10px; margin-right:10px;"></td>
	    <td width="305"><span class="ddtp_3"><img src="${wine.picture }" width="59" height="58" /></span><span class="ddmc_xq"><a href="#"><c:out value="${wine.name }"></c:out></a></span></td>
	    
	    <td width="116"><c:out value="${wine.kind }"></c:out></td>
	    <td width="114" style="color:#F00;">￥<c:out value="${wine.price }"></c:out></td>
	    <td width="147"><input name="" value="-" type="button" / style="background: url(images/jia.jpg); width:19px; height:19px;">
	                    <input type="text" value="${sessionScope.ShoppingCar.get(wine)}" / style="width:40px; text-align:center; height:18px; border:1px solid #dddddd;" >  
	                    <input name="" value="+" type="button" / style="background: url(images/jia.jpg); width:19px; height:19px;"></td>
	    
	    <fmt:parseNumber var="wp" integerOnly="true" type="number" value="${wine.price}" />
	    <fmt:formatNumber var="total" type="number" groupingUsed="false" maxFractionDigits="2" value="${wine.price*sessionScope.ShoppingCar.get(wine)}" />
	    <td width="83">+<c:out value="${wp }"></c:out></td>
	    <c:set var="totall" value="${wine.price }*${sessionScope.ShoppingCar.get(wine)}" ></c:set>
	    <td width="118" style="color:#F00;">￥<c:out value="${total}"></c:out></td>
	    <td width="72"><a href="ShoppingCarServlet?method=delOne&wineid=<c:out value="${wine.id }"></c:out>">删除</a></td>
	  </tr>
	</table>
	</div>
	<c:set var="num" value="${1+num }"></c:set>
	<fmt:formatNumber type="number" groupingUsed="false"  var="money" value="${total+money }"></fmt:formatNumber>
	
</c:forEach>


<div class="jxgw_zj">

<div class="jxgw_wq"><a href="ShoppingCarServlet?method=delAll">清空购物车</a></div>
<div class="zjs2">
<div class="none" style="height:15px;"></div>
<div class="zjs2_1">共<span style="color:#F00; font-size:14px;"><c:out value="${num }"></c:out></span>件商品，商品总价：<span style="color:#F00; font-size:15px;">￥<c:out value="${money}"></c:out></span></div>
<div class="zjs2_1">应付（含运费）：<span style="color:#F00; font-size:15px;">￥<c:out value="${money}"></c:out></span></div>

<div class="qjs"><a href="ddqr.jsp">去结算</a></div>
</div>

</div>


</c:if>





<!--浏览记录开始-->

<div class="lljl_2">
<div class="lljl_bt1"><span class="lljl_mc">浏览记录</span></div>

<div class="lljl_tp1">
<div class="lljl_tp2"><a href="#"><img src="images/lljl1.jpg" width="180" height="178" /></a></div>
<div class="lljl_tp2_mc"><a href="#">樱桃爷爷 综合凤梨酥 台湾特产500g 原味、桂圆和核桃</a>	</div>
<div class="lljl_tp2_jg"><span class="jia_jg">￥17.5</span></div>
</div>


<div class="lljl_tp1">
<div class="lljl_tp2"><a href="#"><img src="images/lljl1.jpg" width="180" height="178" /></a></div>
<div class="lljl_tp2_mc"><a href="#">樱桃爷爷 综合凤梨酥 台湾特产500g 原味、桂圆和核桃</a>	</div>
<div class="lljl_tp2_jg"><span class="jia_jg">￥17.5</span></div>
</div>


<div class="lljl_tp1">
<div class="lljl_tp2"><a href="#"><img src="images/lljl1.jpg" width="180" height="178" /></a></div>
<div class="lljl_tp2_mc"><a href="#">樱桃爷爷 综合凤梨酥 台湾特产500g 原味、桂圆和核桃</a>	</div>
<div class="lljl_tp2_jg"><span class="jia_jg">￥17.5</span></div>
</div>



<div class="lljl_tp1">
<div class="lljl_tp2"><a href="#"><img src="images/lljl1.jpg" width="180" height="178" /></a></div>
<div class="lljl_tp2_mc"><a href="#">樱桃爷爷 综合凤梨酥 台湾特产500g 原味、桂圆和核桃</a>	</div>
<div class="lljl_tp2_jg"><span class="jia_jg">￥17.5</span></div>
</div>



<div class="lljl_tp1">
<div class="lljl_tp2"><a href="#"><img src="images/lljl1.jpg" width="180" height="178" /></a></div>
<div class="lljl_tp2_mc"><a href="#">樱桃爷爷 综合凤梨酥 台湾特产500g 原味、桂圆和核桃</a>	</div>
<div class="lljl_tp2_jg"><span class="jia_jg">￥17.5</span></div>
</div>








</div>

<!--浏览记录结束-->



</div>

